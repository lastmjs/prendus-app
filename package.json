{
  "name": "prendus-graphql-demo",
  "version": "0.1.17",
  "description": "Testing ground for the future of GraphQL with Prendus",
  "scripts": {
    "start": "cd src && node_modules/.bin/zwitterion --target ES2015 --watch-files --ts-warning",
    "test": "exit 0",
    "pull-graphcool": "graphcool pull --project cj36de9q4dem00134bhkwm44r",
    "postinstall": "npm install -g bower && bower install && cd src && npm install",
    "attribution": "generate-attribution && cd src && generate-attribution",
    "build-static": "cd src && node_modules/.bin/zwitterion --target ES2015 --build-static --exclude-dirs bower_components"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lastmjs/prendus-graphql-demo.git"
  },
  "author": "Jordan Last <jordan.michael.last@gmail.com>",
  "bugs": {
    "url": "https://github.com/lastmjs/prendus-graphql-demo/issues"
  },
  "homepage": "https://github.com/lastmjs/prendus-graphql-demo#readme",
  "dependencies": {},
  "devDependencies": {
    "graphcool": "^1.2.1",
    "oss-attribution-generator": "^1.2.0"
  }
}
